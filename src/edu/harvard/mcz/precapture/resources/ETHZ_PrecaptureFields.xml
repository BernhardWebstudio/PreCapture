        <tns:Optional>false</tns:Optional>
        <tns:UseItalic>false</tns:UseItalic>
        <tns:NewLine>false</tns:NewLine>
        <tns:FontSize>12</tns:FontSize>
        <tns:DefaultValue></tns:DefaultValue>
    </tns:FieldInList>
    <tns:FieldInList>
        <tns:Label>Infraspecific Epithet</tns:Label>
        <tns:Code>i</tns:Code>
        <tns:Type>String</tns:Type>
        <tns:VocabularyTerm>dwc:infraspecificEpithet</tns:VocabularyTerm>
        <tns:Optional>false</tns:Optional>
        <tns:UseItalic>true</tns:UseItalic>
        <tns:NewLine>false</tns:NewLine>
        <tns:FontSize>12</tns:FontSize>
        <tns:DefaultValue></tns:DefaultValue>
    </tns:FieldInList>
    <tns:FieldInList>
        <tns:Label>Authorship</tns:Label>
        <tns:Code>a</tns:Code>
        <tns:Type>String</tns:Type>
        <tns:VocabularyTerm>dwc:scientificNameAuthorship</tns:VocabularyTerm>
        <tns:Optional>false</tns:Optional>
        <tns:UseItalic>false</tns:UseItalic>
        <tns:NewLine>true</tns:NewLine>
        <tns:FontSize>12</tns:FontSize>
        <tns:DefaultValue></tns:DefaultValue>
    </tns:FieldInList>
    <tns:FieldInList>
        <tns:Label>Collection</tns:Label>
