    </tns:FieldInList>
    <tns:FieldInList>
        <tns:Label>Sex</tns:Label>
        <tns:Code>x</tns:Code>
        <tns:Type>String</tns:Type>
        <tns:VocabularyTerm>dwc:sex</tns:VocabularyTerm>
